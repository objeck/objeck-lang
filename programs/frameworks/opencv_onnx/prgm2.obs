use API.Onnx, Data.JSON, API.OpenCV, System.IO.Filesystem, Collection;

class Test {
	# Nvidia DML 650-750 ms
	function : Main(args : String[]) ~ Nil {
		if(args->Size() = 1) {
			capture := VideoCapture->New(args[0]);
			if(capture->IsOpen()) {
				capture->Get(VideoCapture->Property->CAP_PROP_FRAME_COUNT)->PrintLine();
				capture->Set(VideoCapture->Property->CAP_PROP_POS_MSEC, 0.75);
				capture->Release();
			};
		};
	}
}