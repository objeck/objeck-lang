class Test {
	function : Main(args : String[]) ~ Nil {
		each(i : 10) {
			System.IO.Standard->Print("int_fact=")->Print(Int->Factorial(i))->Print(", float_fact=")->PrintLine(Float->Factorial(i->As(Float)));
		};

		System.IO.Standard->Print("float_fact=")->PrintLine(Float->Factorial(-2.5));
		System.IO.Standard->Print("float_double_fact=")->PrintLine(Float->DoubleFactorial(6));
		System.IO.Standard->Print("float_gamma=")->PrintLine(Float->Gamma(2.5));
	}
}
