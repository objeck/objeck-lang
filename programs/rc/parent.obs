class Fie {
	@id : Int;
	New() { @id := 13; }
}

class Foo from Fie {
	@id : Int;
	New() { Parent(); @id := 7; }

	method : public : GetMyId() ~ Int {
		return @self->@id;
	}
	
	method : public : GetParentId() ~ Int {
		return @parent->@id;
	}
}

class Fum {
	function : Main(args : String[]) ~ Nil {
		b := Foo->New();
		(b->GetMyId() + b->GetParentId())->PrintLine();
	}
}

