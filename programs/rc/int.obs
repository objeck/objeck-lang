use System.IO;

bundle Default {
  class Arithmetic {
    function : Main(args : String[]) ~ Nil {
      DoArithmetic();
    }
	
    function : native : DoArithmetic() ~ Nil {
      a := Console->ReadString()->ToInt();
      b := Console->ReadString()->ToInt();
  
      Console->Print("a+b = ")->PrintLine(a+b);
      Console->Print("a-b = ")->PrintLine(a-b);
      Console->Print("a*b = ")->PrintLine(a*b);
      Console->Print("a/b = ")->PrintLine(a/b);
    }
  }
}
