bundle Default {
	class Test {
		function : Main(args : String[]) ~ Nil {
			Run();
		}

		function : native : Run() ~ Nil {
			f := IO.File.FileReader->New("in.txt");
			if(f->IsOpen()) {
				string := f->ReadString();
				while(f->IsEOF() = false) {
					string->PrintLine();
					string := f->ReadString();
				};
				f->Close();
			};
		}			
	}
}
