use System;

bundle Default {
	class Test {
		function : Main(args : System.String[]) ~ Nil {
			Run();
		}

		function : native : Run() ~ Nil {
			i := 10;
			ii := Int->New[3];
			ii[0] := 13;
			ii[1] := 17;
			ii[2] := 10;
			i := ii[0] + ii[2];

			Value()->PrintLine();	
			i->PrintLine();
		}

		function : Value() ~ Int {
			x := 10;
			return x + 3;
		}
	}
}
