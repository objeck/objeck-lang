bundle Default {
	class RandomNumbers {
		function : Main(args : String[]) ~ Nil {
			i := 0;
			while(i < 10) {
				if(i = 5) {
					break;
				};
				i->PrintLine();
				i += 1;
			};

			for(i := 0; i < 10; i += 1;) {
				if(i = 5) {
					break;
				};
				i->PrintLine();
			};

			i := 0;
			do {
				if(i = 5) {
					break;
				};
				i->PrintLine();
				i += 1;
			}
			while(i < 10);
		}
	}
}
