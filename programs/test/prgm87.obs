bundle Default {
	enum Widget {
		A, B, C
	}

	enum Car {
		A, B, C
	}

	class Thingy {
		New() {
		}

		method : public : GetThing() ~ Widget {
			return Widget->A;
		}
	}

	class Test {
		function : MakeThingy() ~ Thingy {
			return Thingy->New();
		}

		function : Main(args : String[]) ~ Nil {
			if(MakeThingy()->GetThing() = Widget->A) {
				"is a"->PrintLine();
			}
			else {
				"not a"->PrintLine();
			};
		}
	}
}
