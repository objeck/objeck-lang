bundle Default {
	class Blah {
		@value : static : Int;
		New() {
			if(@value = 0) {
				@value := 17;
				"not set"->PrintLine();
			}
			else {
				"set"->PrintLine();
			};
		}

		method : public : Get() ~ Int {
			return @value;
		}
	}

	class Test {
		function : Main(args : System.String[]) ~ Nil {
			DoStuff();
		}
			
		function : native : DoStuff() ~ Nil {
			b : Blah := Blah->New();
			b->Get()->PrintLine();

			bb : Blah := Blah->New();
			bb->Get()->PrintLine();
		}
	}
}
