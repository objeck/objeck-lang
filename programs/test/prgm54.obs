use System.IO.Net;
use Collection;
use HTTP;

bundle Default {
	class HttpTest {
		function : Main(args : String[]) ~ Nil {
			if(args->Size() = 1) {
				client := HttpClient->New();
				lines := client->Get(args[0], 80);
				
				for(i := 0; i < lines->Size(); i := i + 1;) {
					lines->Get(i)->As(String)->PrintLine();
				};
			};
		}
	}
}
