bundle Default {
	class BasicBlocks {
		function : Main(args : System.String[]) ~ Nil {
			Run(8)->PrintLine();
		}

		function : public : Run(v : Int) ~ Int {
			if(v < 10) {
				v *= v;
			}
			else {
				a := 6;
				b := "Hello";
				v *= (a + b->Size());
			};

			z := v - 3;
			y := -1;
			
			return v * y;
		}
	}
}
