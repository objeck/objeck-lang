bundle Default {
	class Test {
		function : Main(args : System.String[]) ~ Nil {
			Run();
			"Hello"->PrintLine();
		}
		
		function : Run() ~ Nil {
#		function : native : Run() ~ Nil {
			aa : Int[,,] := Int->New[3,5,2];			
			

#			aa : Int[,] := Int->New[3,10];

			value : Int := 0;
			for(i : Int := 0; i < 3; i := i + 1;) {
				for(j : Int := 0; j < 5; j := j + 1;) {
					for(k : Int := 0; k < 2; k := k + 1;) {
						aa[i,j,k] := value;		
						value := value + 1;
					};
				};
			};
			
			PrintLine(aa);				

		}

		function : PrintLine(aa : Int[,,]) ~ Nil {
#		function : native : PrintLine(aa : Int[,,]) ~ Nil {
			for(i : Int := 0; i < 3; i := i + 1;) {
				for(j : Int := 0; j < 5; j := j + 1;) {
					for(k : Int := 0; k < 2; k := k + 1;) {
						aa[i,j,k]->PrintLine();
					
						if(aa[i,j,k] % 2 = 0) {
							aa[i,j,k]->PrintLine();
						}
						else {
							v : Int := aa[i,j,k];
							select(v) {
								label 3: {
									v->PrintLine();
								}

								label 9: {
									aa[i,j,k]->PrintLine();
								}

								label 27: {
									x : Int := 0;
									y : Int := 8;
									while(x < 30) {
										if(x = 10) {
											x->PrintLine();
											"Ten"->PrintLine();
										}
										else if(x = 11) {
											x->PrintLine();
											"Minus One"->PrintLine();
										}
										else if(x + 5 = 0) {
											x->PrintLine();
											"Zero"->PrintLine();
										}
										else {
											aa[i,j,k]->PrintLine();
											v->PrintLine();
										};
										x := x + 1;
									};
								}
							};
						};
					};
				};
			};
		}
	}
}
