use System;

bundle Default {
	class Test {
		function : Main(args : System.String[]) ~ Nil {
			a := 13;
			v : String[] := String->New[3];
			v[0] := String->New("Scan");
			v[1] := String->New("Parse");
			v[2] := String->New("Verify");

			Run(v);
		}
		
		function : Run(args : System.String[]) ~ Nil {
			for(i : Int := 0; i < args->Size(); i := i + 1;) {
				args[i]->PrintLine();
			};
			a->PrintLine();
		}
		a : static : Float;
	}
}	
