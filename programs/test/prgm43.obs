use System.IO;
use System;

bundle Default {
	class Test {
		function : Main(args : System.String[]) ~ Nil {
			"num: "->Print();
			str : String := Console->ReadString();
			if(str->ToInt() * 2 < 34) {
				"less than 34."->PrintLine();
			}
			else {
				"34 or greater."->PrintLine();
			};

			'2'->IsDigit()->PrintLine();
			'a'->IsDigit()->PrintLine();
			'T'->IsChar()->PrintLine();
		}
	}
}
