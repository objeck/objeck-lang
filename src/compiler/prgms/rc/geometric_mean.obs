class ArithmeticMean {
	function : Amg(a : Float, g : Float) ~ Nil {
		a1 := a;
		g1 := g;
		while((a1-g1)->Abs() >= 10.0->Power(-14.0)) {
				tmp := (a1+g1)/2.0;
				g1 := Float->SquareRoot(a1*g1);
				a1 := tmp;
		};
		a1->PrintLine();
	}
	
	function : Main(args : String[]) ~ Nil {
		Amg(1,1/2.0->SquareRoot());
	}
}
