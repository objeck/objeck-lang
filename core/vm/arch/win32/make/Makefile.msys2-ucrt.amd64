ARGS=-O3 -Wall -Wno-unused-function -std=c++17 -Wno-unknown-pragmas -Wno-unused-function -Wno-unused-variable -Wno-dangling-pointer -Wno-int-to-pointer-cast 

AR=ar
SRC=win32.o
LIB=../../win32.a

$(LIB): $(SRC)
	$(AR) -cvq $(LIB) $(SRC)

%.o: %.cpp
	$(CXX) -m64 $(ARGS) -c $< 

clean:
	rm -f *.o *~ $(LIB)
