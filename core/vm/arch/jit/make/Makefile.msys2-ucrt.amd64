ARGS=-O3 -Wall -Wno-unused-function -std=c++17 -Wno-unknown-pragmas -Wno-unused-function -Wno-unused-variable -Wno-int-to-pointer-cast -Wno-maybe-uninitialized

AR=ar
SRC=jit_common.o
LIB=amd64/jit_common.a

$(LIB): $(SRC)
	$(AR) -cvq $(LIB) $(SRC)

%.o: %.cpp
	$(CXX) -m64 $(ARGS) -c $< 

clean:
	rm -f *.o *~ $(LIB)
