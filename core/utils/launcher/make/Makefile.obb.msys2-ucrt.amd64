ARGS=-O3 -Wall -Wno-unused-function -std=c++17

SRC=builder.o
OBJ_LIBS=objeck.res
EXE=obb

$(EXE): $(SRC) $(OBJ_LIBS)
	$(CXX) -m64 -o $(EXE) $(SRC) $(OBJ_LIBS) -lz

%.o: %.cpp
	$(CXX) -m64 $(ARGS) -c $< 

objeck.res:
	windres vs/builder/objeck.rc -O coff -o objeck.res

clean:
	rm -f $(EXE) *.o *~ *.res
