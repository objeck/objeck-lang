      function scrollToNavSection(id) {
        const nav = document.getElementById('side-nav');
        const target = document.getElementById(id);
        if (nav && target) {
          const topPos = target.offsetTop;
          nav.scrollTo({
            top: topPos,
            behavior: 'smooth'
          });
        }
      }
    </script>
